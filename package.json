    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "body-parser": "^1.14.2",
    "chalk": "^1.1.1",
    "compose-middleware": "^2.0.1",
    "compression": "^1.6.0",
    "connect-history-api-fallback": "^1.1.0",
    "connect-redis": "^3.0.2",
    "cookie-parser": "^1.4.1",
    "debug": "^2.2.0",
    "errorhandler": "^1.4.2",
    "express": "^4.13.3",
    "express-session": "^1.13.0",
    "jquery": "^2.2.0",
    "lodash.filter": "^4.0.0",
    "lodash.foreach": "^4.0.0",
    "lodash.intersection": "^4.0.1",
    "lodash.map": "^4.0.2",
    "lodash.mapvalues": "^4.0.0",
    "lodash.omit": "^4.0.1",
    "lodash.values": "^4.0.0",
    "method-override": "^2.3.5",
    "palisade": "^1.0.1",
    "passport": "^0.3.2",
    "passport-facebook": "^2.0.0",
    "pmx": "^0.6.0",
    "redis": "^2.4.2",
    "require-dir": "^0.3.0",
    "rethink-decl": "^1.0.0",
    "rethinkdb-change-stream": "^1.0.0",
    "superagent": "^1.7.1",

    "babel-cli": "^6.4.5",
    "chalk": "^1.1.1",
    "classnames": "^2.2.1",
    "css-loader": "^0.23.0",
    "cssnano": "^3.3.2",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.0.0",
    "eslint-config-standard-react": "^1.0.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-babel": "^3.0.0",

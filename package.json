    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime",
      "add-module-exports"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "standard",
      "standard-react"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "__DEV__": true,
      "__PROD__": true,
      "__INITIAL_STATE__": true
    },
    "rules": {
      "semi": [
        2,
        "never"
      ]
    }
  }
